<SCRIPT mode="1" state="1" tidy="0">
<MSCRI><![CDATA[@echo off

:INC PCK.AUTOMIC_SHARED.PUB.INCLUDE.LOGGING_HEADER_PARAM_VALIDATION

:SET &UC4RB_LOGGING_PARAMETER# = "Start mode"
:INC PCK.AUTOMIC_SHARED.PUB.INCLUDE.LOGGING_PARAM &UC4RB_LOGGING_PARAMETER_VAL# = &UC4RB_START_MODE#

:SET &UC4RB_LOGGING_PARAMETER# = "Service name/Script Filename"
:INC PCK.AUTOMIC_SHARED.PUB.INCLUDE.LOGGING_PARAM &UC4RB_LOGGING_PARAMETER_VAL# = &UC4RB_SERVICE_FILE_NAME#

:INC PCK.AUTOMIC_SHARED.PUB.INCLUDE.LOGGING_HEADER_EXECUTION

!Begin my code
:DEFINE &ARRAY#, string, 20
:FILL &ARRAY#[] = STR_SPLIT(&UC4RB_SERVICE_FILE_NAME#, ",")
:SET &LEN# = LENGTH(&ARRAY#[], SIZE)
:SET &VAR# = 1

@echo off

:WHILE &VAR# LE &LEN#
:  SET &SCRIPT# = STR_TRIM(&ARRAY#[&VAR#])

:  IF &UC4RB_START_MODE# = "script"
     echo INFO: Run script: &SCRIPT#
     call "&SCRIPT#"
:  ELSE
     echo INFO: Run service: &SCRIPT#
     net start "&SCRIPT#"
:  ENDIF

:  INC PCK.AUTOMIC_SHARED.PUB.INCLUDE.CHECK_RETURN_CODE_WINDOWS_CONSOLE
:  SET &VAR# = &VAR# + 1
:ENDWHILE
!End my code]]></MSCRI>
</SCRIPT>
